{
  "name": "Scrap-n-Stick!",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "16": "./assets/icons/icon.png",
    "128": "./assets/icons/icon1.png"
  },
  "browser_action": {
    "default_icon": "./assets/icons/icon1.png",
    "default_popup": "./Popup Page/popup.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "page":"background.html",
    "persistent": false
  }
}
