{
  "name": "Noob Tech",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["storage", "contextMenus", "notifications"],
  "background": {
    "scripts": ["events.js"],
    "persistent": false
  }
}
