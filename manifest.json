{
  "name": "Noob Tech",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "16": "icon1.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon1.png",
    "default_popup": "./Popup Page/popup.html"
  },
  "permissions": ["storage", "contextMenus", "notifications", "tabs"],
  "background": {
    "scripts": ["events.js"],
    "persistent": false
  }
}
